    "perguntas": [
      { "id":"oc1","texto":"Cheguei, vi, venci.","alternativas":["Assindética","Aditiva","Adversativa","Conclusiva"],"correta":0,"tags":["assindetica"]},
      { "id":"oc2","texto":"Acordei cedo, tomei café, saí para o trabalho.","alternativas":["Adversativa","Assindética","Conclusiva","Explicativa"],"correta":1,"tags":["assindetica"]},
      { "id":"oc3","texto":"Correu, tropeçou, caiu.","alternativas":["Adversativa","Assindética","Aditiva","Explicativa"],"correta":1,"tags":["assindetica"]},
      { "id":"oc4","texto":"Viajei, conheci pessoas, fiz amigos.","alternativas":["Assindética","Adversativa","Conclusiva","Alternativa"],"correta":0,"tags":["assindetica"]},
      { "id":"oc5","texto":"Trabalhei, estudei, descansei.","alternativas":["Assindética","Aditiva","Explicativa","Conclusiva"],"correta":0,"tags":["assindetica"]},
      { "id":"oc6","texto":"Pensei, refleti, decidi.","alternativas":["Conclusiva","Assindética","Adversativa","Explicativa"],"correta":1,"tags":["assindetica"]},
      { "id":"oc7","texto":"Abri a porta, entrei, sentei.","alternativas":["Assindética","Alternativa","Explicativa","Adversativa"],"correta":0,"tags":["assindetica"]},
      { "id":"oc8","texto":"Li o livro, fechei, dormi.","alternativas":["Adversativa","Conclusiva","Assindética","Aditiva"],"correta":2,"tags":["assindetica"]},
      { "id":"oc9","texto":"Chegou, sorriu, cumprimentou.","alternativas":["Conclusiva","Assindética","Adversativa","Explicativa"],"correta":1,"tags":["assindetica"]},
      { "id":"oc10","texto":"Falou, convenceu, venceu.","alternativas":["Aditiva","Assindética","Conclusiva","Adversativa"],"correta":1,"tags":["assindetica"]},

      { "id":"oc11","texto":"Estudou bastante e passou no exame.","alternativas":["Assindética","Aditiva","Adversativa","Explicativa"],"correta":1,"tags":["aditiva"]},
      { "id":"oc12","texto":"Gosto de café e de chá.","alternativas":["Aditiva","Conclusiva","Explicativa","Adversativa"],"correta":0,"tags":["aditiva"]},
      { "id":"oc13","texto":"Ele não só estudou, mas também praticou.","alternativas":["Explicativa","Aditiva","Conclusiva","Alternativa"],"correta":1,"tags":["aditiva"]},
      { "id":"oc14","texto":"Estudou e aprendeu.","alternativas":["Adversativa","Aditiva","Conclusiva","Explicativa"],"correta":1,"tags":["aditiva"]},
      { "id":"oc15","texto":"João canta e Maria dança.","alternativas":["Aditiva","Assindética","Adversativa","Explicativa"],"correta":0,"tags":["aditiva"]},
      { "id":"oc16","texto":"Trabalhou de dia e estudou à noite.","alternativas":["Conclusiva","Aditiva","Adversativa","Explicativa"],"correta":1,"tags":["aditiva"]},
      { "id":"oc17","texto":"Nem estudei nem descansei.","alternativas":["Aditiva","Alternativa","Adversativa","Conclusiva"],"correta":0,"tags":["aditiva"]},
      { "id":"oc18","texto":"Correu e caiu.","alternativas":["Adversativa","Explicativa","Aditiva","Conclusiva"],"correta":2,"tags":["aditiva"]},
      { "id":"oc19","texto":"Comprou pão e leite.","alternativas":["Aditiva","Assindética","Adversativa","Conclusiva"],"correta":0,"tags":["aditiva"]},
      { "id":"oc20","texto":"Falou e explicou bem.","alternativas":["Explicativa","Conclusiva","Aditiva","Adversativa"],"correta":2,"tags":["aditiva"]},

      { "id":"oc21","texto":"Estudei bastante, mas não fui bem.","alternativas":["Adversativa","Alternativa","Explicativa","Assindética"],"correta":0,"tags":["adversativa"]},
      { "id":"oc22","texto":"Queria sair, porém estava chovendo.","alternativas":["Conclusiva","Adversativa","Aditiva","Explicativa"],"correta":1,"tags":["adversativa"]},
      { "id":"oc23","texto":"Tentei ajudá-lo, contudo não consegui.","alternativas":["Adversativa","Aditiva","Conclusiva","Explicativa"],"correta":0,"tags":["adversativa"]},
      { "id":"oc24","texto":"Correu muito, entretanto perdeu a corrida.","alternativas":["Alternativa","Explicativa","Adversativa","Conclusiva"],"correta":2,"tags":["adversativa"]},
      { "id":"oc25","texto":"Queria viajar, todavia não tinha dinheiro.","alternativas":["Conclusiva","Adversativa","Aditiva","Explicativa"],"correta":1,"tags":["adversativa"]},
      { "id":"oc26","texto":"Falou, mas não explicou direito.","alternativas":["Explicativa","Aditiva","Adversativa","Conclusiva"],"correta":2,"tags":["adversativa"]},
      { "id":"oc27","texto":"Gostava dele, mas não confiava.","alternativas":["Adversativa","Conclusiva","Explicativa","Aditiva"],"correta":0,"tags":["adversativa"]},
      { "id":"oc28","texto":"Estava cansado, mas continuou.","alternativas":["Adversativa","Aditiva","Conclusiva","Assindética"],"correta":0,"tags":["adversativa"]},
      { "id":"oc29","texto":"Correu rápido, mas perdeu.","alternativas":["Adversativa","Conclusiva","Explicativa","Alternativa"],"correta":0,"tags":["adversativa"]},
      { "id":"oc30","texto":"Estudou, mas não passou.","alternativas":["Aditiva","Adversativa","Conclusiva","Explicativa"],"correta":1,"tags":["adversativa"]},

      { "id":"oc31","texto":"Ou você estuda, ou não passa.","alternativas":["Adversativa","Conclusiva","Alternativa","Explicativa"],"correta":2,"tags":["alternativa"]},
      { "id":"oc32","texto":"Ora ria, ora chorava.","alternativas":["Conclusiva","Adversativa","Alternativa","Aditiva"],"correta":2,"tags":["alternativa"]},
      { "id":"oc33","texto":"Quer viaje, quer fique, será bom.","alternativas":["Adversativa","Alternativa","Conclusiva","Explicativa"],"correta":1,"tags":["alternativa"]},
      { "id":"oc34","texto":"Seja de dia, seja de noite, ele trabalha.","alternativas":["Alternativa","Adversativa","Explicativa","Conclusiva"],"correta":0,"tags":["alternativa"]},
      { "id":"oc35","texto":"Ou falava, ou se calava.","alternativas":["Explicativa","Adversativa","Alternativa","Conclusiva"],"correta":2,"tags":["alternativa"]},
      { "id":"oc36","texto":"Ou estuda, ou trabalha.","alternativas":["Conclusiva","Alternativa","Explicativa","Adversativa"],"correta":1,"tags":["alternativa"]},
      { "id":"oc37","texto":"Ora canta, ora dança.","alternativas":["Adversativa","Conclusiva","Alternativa","Explicativa"],"correta":2,"tags":["alternativa"]},
      { "id":"oc38","texto":"Quer chova, quer faça sol, iremos.","alternativas":["Conclusiva","Alternativa","Adversativa","Explicativa"],"correta":1,"tags":["alternativa"]},
      { "id":"oc39","texto":"Ou fala, ou se cala.","alternativas":["Adversativa","Alternativa","Conclusiva","Explicativa"],"correta":1,"tags":["alternativa"]},
      { "id":"oc40","texto":"Seja certo, seja errado, vou agir.","alternativas":["Conclusiva","Alternativa","Explicativa","Adversativa"],"correta":1,"tags":["alternativa"]},

      { "id":"oc41","texto":"Estudou bastante, portanto passou.","alternativas":["Conclusiva","Explicativa","Aditiva","Assindética"],"correta":0,"tags":["conclusiva"]},
      { "id":"oc42","texto":"Estudou, logo passou.","alternativas":["Adversativa","Conclusiva","Explicativa","Aditiva"],"correta":1,"tags":["conclusiva"]},
      { "id":"oc43","texto":"Esforçou-se muito, assim venceu.","alternativas":["Conclusiva","Aditiva","Explicativa","Adversativa"],"correta":0,"tags":["conclusiva"]},
      { "id":"oc44","texto":"Fez tudo certo, logo ganhou.","alternativas":["Explicativa","Conclusiva","Adversativa","Aditiva"],"correta":1,"tags":["conclusiva"]},
      { "id":"oc45","texto":"Treinou, pois venceu.","alternativas":["Conclusiva","Adversativa","Aditiva","Explicativa"],"correta":0,"tags":["conclusiva"]},
      { "id":"oc46","texto":"Esforçou-se, portanto conseguiu.","alternativas":["Conclusiva","Explicativa","Adversativa","Aditiva"],"correta":0,"tags":["conclusiva"]},
      { "id":"oc47","texto":"Trabalhou muito, logo descansou.","alternativas":["Explicativa","Conclusiva","Adversativa","Aditiva"],"correta":1,"tags":["conclusiva"]},
      { "id":"oc48","texto":"Ele estudou, pois sabia da prova.","alternativas":["Conclusiva","Explicativa","Adversativa","Aditiva"],"correta":0,"tags":["conclusiva"]},
      { "id":"oc49","texto":"Cometeu erros, portanto aprendeu.","alternativas":["Conclusiva","Adversativa","Explicativa","Aditiva"],"correta":0,"tags":["conclusiva"]},
      { "id":"oc50","texto":"Leu bastante, assim escreveu bem.","alternativas":["Explicativa","Conclusiva","Adversativa","Aditiva"],"correta":1,"tags":["conclusiva"]},

      { "id":"oc51","texto":"Não vá, porque está perigoso.","alternativas":["Conclusiva","Explicativa","Adversativa","Alternativa"],"correta":1,"tags":["explicativa"]},
      { "id":"oc52","texto":"Saia cedo, pois o trânsito estará ruim.","alternativas":["Explicativa","Conclusiva","Adversativa","Alternativa"],"correta":0,"tags":["explicativa"]},
      { "id":"oc53","texto":"Não faça barulho, que estou cansado.","alternativas":["Explicativa","Conclusiva","Adversativa","Aditiva"],"correta":0,"tags":["explicativa"]},
      { "id":"oc54","texto":"Volte logo, porque preciso falar com você.","alternativas":["Conclusiva","Explicativa","Adversativa","Aditiva"],"correta":1,"tags":["explicativa"]},
      { "id":"oc55","texto":"Não grite, pois estou doente.","alternativas":["Explicativa","Conclusiva","Adversativa","Alternativa"],"correta":0,"tags":["explicativa"]},
      { "id":"oc56","texto":"Saia já, porque estou mandando.","alternativas":["Explicativa","Adversativa","Conclusiva","Aditiva"],"correta":0,"tags":["explicativa"]},
      { "id":"oc57","texto":"Cale-se, que já é tarde.","alternativas":["Conclusiva","Explicativa","Adversativa","Aditiva"],"correta":1,"tags":["explicativa"]},
      { "id":"oc58","texto":"Durma, pois amanhã acordará cedo.","alternativas":["Conclusiva","Explicativa","Adversativa","Aditiva"],"correta":1,"tags":["explicativa"]},
      { "id":"oc59","texto":"Vá devagar, que a estrada é perigosa.","alternativas":["Adversativa","Conclusiva","Explicativa","Aditiva"],"correta":2,"tags":["explicativa"]},
      { "id":"oc60","texto":"Não saia, porque está chovendo.","alternativas":["Conclusiva","Explicativa","Adversativa","Aditiva"],"correta":1,"tags":["explicativa"]}
    ]
